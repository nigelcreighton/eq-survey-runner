{
    "mime_type": "application/json/ons/eq",
    "schema_version": "0.0.1",
    "data_version": "0.0.2",
    "survey_id": "001",
    "title": "Test Routing Number Less Than",
    "theme": "default",
    "description": "A test survey for routing based on a number less than",
    "metadata": [{
        "name": "user_id",
        "validator": "string"
    }, {
        "name": "period_id",
        "validator": "string"
    }, {
        "name": "ru_name",
        "validator": "string"
    }],
    "sections": [{
        "id": "default-section",
        "groups": [{
            "blocks": [{
                "type": "Question",
                "id": "number-question",
                "description": "",
                "question": {
                    "answers": [{
                        "id": "answer",
                        "mandatory": true,
                        "type": "Number",
                        "label": "Less than 123"
                    }],
                    "id": "question",
                    "title": "Enter the number less than 123",
                    "type": "General"
                },
                "title": "Number Routing Less Than",
                "routing_rules": [{
                    "goto": {
                        "block": "correct-answer",
                        "when": [{
                            "id": "answer",
                            "condition": "less than",
                            "value": 123
                        }]
                    }
                }, {
                    "goto": {
                        "block": "incorrect-answer"
                    }
                }]
            }, {
                "type": "Interstitial",
                "id": "incorrect-answer",
                "title": "You entered a number that was not less than 123",
                "content": [{
                    "description": "You were asked to enter a number less than <em>123</em> but you entered <em>{{answers['answer']}}</em>."
                }],
                "routing_rules": [{
                    "goto": {
                        "block": "summary"
                    }
                }]
            }, {
                "type": "Interstitial",
                "id": "correct-answer",
                "title": "You entered a number less than 123",
                "content": [{
                    "description": "You were asked to enter a number less than <em>123</em> and you entered <em>{{answers['answer']}}</em>."
                }]
            }, {
                "type": "Summary",
                "id": "summary"
            }],
            "id": "group",
            "title": ""
        }]
    }]
}
